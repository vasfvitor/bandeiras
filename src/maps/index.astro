---
import BR from "~/maps/BR.svg";
---
<section class="content drop-shadow-lg bg-transparent h-[75vh] bg-blue-900 fill-zinc-800">
  <div class="map-wrapper">
    <div id="toolTip"></div>
    <object id="map" data={BR} type="image/svg+xml"></object>
  </div>
  <div class="h-8 bg-yellow-400"></div>
  <div class="h-8 bg-blue-800"></div>
  <div class="h-8 bg-white"></div>
  <div class="h-8 bg-green-800"></div>
  
</section>
<script is:inline src="/BR-map.js"></script>
